{
  "name": "ai-code-commenter",
  "displayName": "AI Code Commenter",
  "version": "0.0.1",
  "publisher": "ankushajari",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-code-commenter"
  },
  "engines": { "vscode": "^1.90.0" },
  "activationEvents": ["onCommand:aiCommenter.addComments"],
  "main": "./dist/extensionwithCodeLens.js",
  "scripts": { "compile": "tsc -p ./", "watch": "tsc -watch -p ./" },
  "contributes": {
    "commands": [
      { 
        "command": "aiCommenter.addComments", 
        "title": "AI: Add Comments to Code" 
      },
      {
        "command": "aiCommenter.updateProjectContext",
        "title": "AI: Build Project Context"
      }
    ]
  },
  "dependencies": { "axios": "^1.6.7" },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "typescript": "^5.3.3",
    "vsce": "^2.15.0"
  }
}